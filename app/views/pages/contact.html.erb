    <div class="container" id="main-content">
      <div class="row">
        <section class="span12">
          <header class="out">
            <h2>Contactanos</h2>
          </header>

          <div class="wrapper-padding">

            <div class="wrapper">
              <article class="row-fluid">
                <div class="span8">
                  <%= form_for(Message.new(), :url => { :action => "contact_potential_advertiser", :controller => :pages }, :html => { :method => :post }) do |f| %>
                      <fieldset>
                        <p>Por favor, ponete en contacto con nosotros mediante el formulario.</p>
                        <hr>
                        <div class="row-fluid">
                          <div class="span6">
                            <label>Nombre</label>
                            <%=f.text_field :name, :label => false, :class => "input-xlarge", :style => "margin-right:10px;" %>

                            <label>Email</label>
                            <%=f.text_field :email, :label => false, :class => "input-xlarge", :style => "margin-right:10px;" %>
                          </div>
                          <div class="span6" style="margin-top: 66.5px">
                            <label>Asunto</label>
                            <%=f.text_field :subject, :label => false, :class => "input-xlarge", :style => "margin-right:10px;" %>
                          </div>
                        </div>

                        <label>Comentarios</label>
                        <%=f.text_area :body, :label => false, :rows => "3", :style => "width:97%;" %>

                        <hr>
                        <div class="text-center">
                          <button type="submit" class="btn btn-dealhunter">Enviar</button>
                        </div>
                      </fieldset>
                  <% end %>
                </div> <!--end span8-->
                <div class="span3 offset1">
